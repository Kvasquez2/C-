        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Windows"
        mc:Ignorable="d"
        Title="Select" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnBack" Content="Back" HorizontalAlignment="Left" Margin="57,352,0,0" VerticalAlignment="Top" Height="52" Width="112" Click="btnBack_Click"/>
        <Label Content="Select" HorizontalAlignment="Left" Margin="355,106,0,0" VerticalAlignment="Top" FontSize="25"/>
        <Label Content="Proveedor" HorizontalAlignment="Left" Margin="215,190,0,0" VerticalAlignment="Top" FontSize="20"/>
